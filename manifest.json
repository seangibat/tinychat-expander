{
  "name": "TinyChatWindowExpander",
  "short_name": "TCWE",
  "manifest_version": 2,
  "version":"1.0",
  "description": "Expands the TinyChat window to fill the screen.",
  "permissions": ["tabs", "http://tinychat.com/*"],
  "content_scripts": [
    {
      "matches": ["http://tinychat.com/*"],
      "js": ["expander.js"]
    }
  ]
}